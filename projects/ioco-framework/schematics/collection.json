{
    "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
    "schematics": {
        "ng-add": {
            "description": "Add IOCO framework to your projects",
            "factory": "./ng-add/index#ngAdd"
        },
        "app-settings": {
            "description": "Adds IOCO default app settings",
            "factory": "./app-settings/index#addAppSettings"
        },
        "azure-pipeline": {
            "description": "Adds IOCO default azure pipeline",
            "factory": "./azure-pipeline/index#addAzurePipeline"
        },
        "arm-templates": {
            "description": "Adds IOCO bicep files",
            "factory": "./arm-templates/index#addBicepFiles"
        },
        "components": {
            "description": "Adds IOCO default components",
            "factory": "./components/index#addComponents"
        },
        "dialog-boxes": {
            "description": "Adds IOCO default dialog boxes",
            "factory": "./dialog-boxes/index#addDialogBoxes"
        },
        "docker": {
            "description": "Adds IOCO docker files",
            "factory": "./docker/index#addDocker"
        },
        "endpoints": {
            "description": "Adds IOCO default endpoint configuration",
            "factory": "./endpoints/index#addEndpoints"
        },
        "enums": {
            "description": "Adds IOCO default enums",
            "factory": "./enums/index#addEnums"
        },
        "helpers": {
            "description": "Adds IOCO default helpers",
            "factory": "./helpers/index#addHelpers"
        },
        "interceptors": {
            "description": "Adds IOCO default interceptors",
            "factory": "./interceptors/index#addInterceptors"
        },
        "karma-config": {
            "description": "Adds IOCO karma configuration",
            "factory": "./karma/index#addKarmaConfiguration"
        },
        "main-file": {
            "description": "Adds IOCO main file configuration",
            "factory": "./main/index#addMainFileConfiguration"
        },
        "material-design": {
            "description": "Adds material-design module to project",
            "factory": "./modules/index#addMaterialDesign"
        },
        "msal": {
            "description": "Adds microsoft authentication library module to project",
            "factory": "./modules/index#addMSAL"
        },
        "mock-data": {
            "description": "Adds IOCO default mock-data",
            "factory": "./mock-data/index#addMockData"
        },
        "model-helpers": {
            "description": "Adds IOCO model helpers",
            "factory": "./models/index#addModelHelpers"
        },
        "service": {
            "description": "Adds service with IOCO default structure",
            "factory": "./service/index#GenerateNewService",
            "schema": "./service/schema.json",
            "aliases": ["is"]
        },
        "services": {
            "description": "Adds IOCO default services",
            "factory": "./services/index#addServices"
        },
        "unit-test-helpers": {
            "description": "Adds IOCO default unit test helpers",
            "factory": "./unit-test-helpers/index#addUnitTestHelpers"
        },
        "vscode": {
            "description": "Adds IOCO .vscode folder with settings",
            "factory": "./vscode/index#addVSCode"
        },
        "web-config": {
            "description": "Adds IOCO default web.config",
            "factory": "./web-config/index#addWebConfig"
        }
    }
}