{
  "name": "@ioco-dev/ioco-framework",
  "version": "1.1.0",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles \"schematics/collection.json\" \"schematics/app-settings/app-settings-root/**/*\" \"schematics/arm-templates/arm-templates-root/**/*\" \"schematics/azure-pipeline/azure-pipeline-root/**/*\" \"schematics/components/components-root/**/*\" \"schematics/dialog-boxes/dialog-boxes-root/**/*\" \"schematics/docker/docker-root/**/*\" \"schematics/endpoints/endpoints-root/**/*\" \"schematics/enums/enums-root/**/*\" \"schematics/helpers/helpers-root/**/*\" \"schematics/interceptors/interceptors-root/**/*\" \"schematics/karma/karma-root/**/*\" \"schematics/mock-data/mock-data-root/**/*\" \"schematics/models/models-root/**/*\" \"schematics/modules/modules-root/**/*\" \"schematics/service/new-service-root/**/*\" \"schematics/service/schema.json\"  \"schematics/services/services-root/**/*\" \"schematics/unit-test-helpers/unit-test-helpers-root/**/*\" \"schematics/web-config/web-config-root/**/*\" \"../../dist/ioco-framework\""
  },
  "ng-add": {
    "save": "true"
  },
  "schematics": "./schematics/collection.json",
  "peerDependencies": {
    "@angular/common": "latest",
    "@angular/core": "latest",
    "@angular/material": "latest",
    "@angular/flex-layout": "latest",
    "@angular/cdk": "latest",
    "@angular-eslint/schematics": "latest"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}